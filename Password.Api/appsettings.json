{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Password": {
    "MinLength": 9,
    "MaxLength": 30,
    "MinNumber": 1,
    "MinUpperCase": 1,
    "MinLowerCase": 1,
    "MinSymbol": 1,
    "CharSymbol": "!@#$%^&*()\\-+",
    "RepeatChar": false
  }
}
